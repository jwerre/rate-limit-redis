{
	"name": "@jwerre/rate-limit-redis",
	"version": "2.0.0",
	"description": "A Redis based rate limiter for Node.js and Express.",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "rm -rf dist && rollup -c",
		"check": "npm run lint && npm run spell && npm run test",
		"lint": "eslint .",
		"release": "semantic-release",
		"release:dry": "semantic-release --dry-run",
		"spell": "cspell .",
		"test": "mocha test --exit"
	},
	"repository": {
		"type": "git",
		"url": "github:jwerre/rate-limit-redis"
	},
	"keywords": [
		"redis",
		"ratelimit",
		"rate",
		"limit",
		"429",
		"express",
		"expressjs",
		"cache",
		"x-ratelimit-limit",
		"x-ratelimit-remaining",
		"retry-after",
		"headers"
	],
	"author": "Jonah Werre <jonahwerre@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jwerre/rate-limit-redis/issues"
	},
	"homepage": "https://github.com/jwerre/rate-limit-redis/blob/master/README.md",
	"dependencies": {
		"redis": "^5.8.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/github": "^11.0.4",
		"@semantic-release/npm": "^12.0.2",
		"@semantic-release/release-notes-generator": "^14.0.3",
		"cspell": "^9.2.0",
		"eslint": "^9.33.0",
		"express": "^5.1.0",
		"mocha": "^11.7.1",
		"rollup": "^4.46.3",
		"semantic-release": "^24.2.7",
		"supertest": "^7.1.4"
	}
}
